#ifndef SPICOMMANDSOVERSEER_H_
#define SPICOMMANDSOVERSEER_H_

#define START_MOTOR_1	0x01
#define START_MOTOR_2	0x02
#define START_MOTOR_3	0x03
#define START_MOTOR_4	0x04
#define STOP_MOTOR_1	0x05
#define STOP_MOTOR_2	0x06
#define STOP_MOTOR_3	0x07
#define STOP_MOTOR_4	0x08




#endif /* SPICOMMANDSOVERSEER_H_ */
